{
  "version": 1,
  "serviceConfiguration": {
    "apiName": "roshub",
    "serviceName": "AppSync",
    "defaultAuthType": {
      "mode": "AMAZON_COGNITO_USER_POOLS",
      "cognitoUserPoolId": "authRoshub"
    },
    "additionalAuthTypes": [
      {
        "mode": "API_KEY",
        "expirationTime": 365,
        "apiKeyExpirationDate": "2023-09-07T12:17:57.926Z",
        "keyDescription": ""
      }
    ],
    "conflictResolution": {
      "defaultResolutionStrategy": {
        "type": "AUTOMERGE"
      },
      "perModelResolutionStrategy": [
        {
          "resolutionStrategy": {
            "type": "AUTOMERGE"
          },
          "entityName": "User"
        },
        {
          "resolutionStrategy": {
            "type": "AUTOMERGE"
          },
          "entityName": "Request"
        },
        {
          "resolutionStrategy": {
            "type": "AUTOMERGE"
          },
          "entityName": "Service"
        },
        {
          "resolutionStrategy": {
            "type": "AUTOMERGE"
          },
          "entityName": "RoshubPoint"
        },
        {
          "resolutionStrategy": {
            "type": "AUTOMERGE"
          },
          "entityName": "Payment"
        }
      ]
    }
  }
}